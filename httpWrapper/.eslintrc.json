{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": ["error", "never"],
    "object-curly-newline": [
      "error",
      {
        "consistent": true,
        "multiline": true
      }
    ],
    "radix": ["error", "as-needed"],
    "no-param-reassign": ["error", { "props": false }],
    "max-len": ["error", { "code": 100, "ignoreComments": true }],
    "consistent-return": "off",
    "no-console": "off"
  }
}
